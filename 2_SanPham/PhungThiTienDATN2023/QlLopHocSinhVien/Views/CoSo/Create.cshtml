@model QlLopHocSinhVien.Model.Entities.CoSo
@{
    ViewData["Title"] = "Create";
}

<form href="/CoSo/Create" method="post">
    <div class="form-group">
        <lable asp-for="MaCs">Mã cơ sởc</lable>
        <input asp-for="MaCs" class="form-control" />

        <span asp-validation-for="MaCs" class="text-danger"></span>

    </div>
    <div class="form-group">
        <lable asp-for="TenCs">Tên cơ sở</lable>
        <input asp-for="TenCs" class="form-control" />
        <span asp-validation-for="TenCs" class="text-danger"></span>
    </div>

    <div class="form-group">
        <lable asp-for="DiaChi">Địa chỉ</lable>
        <input asp-for="DiaChi" class="form-control" />
        <span asp-validation-for="DiaChi" class="text-danger"></span>
    </div>
   


    <div class="form-group">
        <button class="btn btn-success">Lưu</button>
    </div>

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
</form>
